{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "esnext",
      "es2015.collection",
      "es2015.iterable"
    ],
    "allowJs": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
